programa {
  real numero1, numero2, soma_resulatdo
  funcao inicio() {
    escreva("Número decimal")
    leia(numero1)
    escreva("O segundo número decimal")
    leia(numero2)
    soma_resulatdo = numero1 + numero2
    escreva("A soma do numero 1 ",numero1,"e",numero2,"e igual a :",soma_resulatdo,"\n")
  }
}
